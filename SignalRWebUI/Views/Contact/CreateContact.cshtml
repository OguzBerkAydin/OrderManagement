@model CreateContactDto

@{
	ViewData["Title"] = "CreateContact";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">İletişim İşlemleri</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">


					<div class="card">
						<div class="card-header">
							<div class="card-title">Yeni İletişim Girişi</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="Location">Lokayson Girin</label>
								<input type="text" class="form-control" asp-for="Location" id="Location">

							</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="Phone">Telefon No Girin</label>
								<input type="text" class="form-control" asp-for="Phone" id="Phone">

							</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="Mail">e-mail adresi Girin</label>
								<input type="email" class="form-control" asp-for="Mail" id="Mail">

							</div>
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="FooterDescription">Açıklama Girin</label>
								<input type="text" class="form-control" asp-for="FooterDescription" id="FooterDescription">

							</div>
						</div>
						<div class="card-action">
							<button class="btn btn-primary">İletişim  Ekle</button>

						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>


